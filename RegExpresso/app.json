{
	"expo": {
		"name": "RegExpresso",
		"slug": "regexpresso",
		"scheme": "regexpresso",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/adaptive-icon.png",
		"userInterfaceStyle": "light",
		"newArchEnabled": true,
		"splash": {
			"image": "./assets/images/splash.png",
			"resizeMode": "contain",
			"backgroundColor": "#e8cdad"
		},
		"ios": {
			"supportsTablet": true,
			"config": {
				"usesNonExemptEncryption": false
			}
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/adaptive-icon.png",
				"backgroundColor": "#e8cdad"
			},
			"package": "com.triplethreat.regexpresso"
		},
		"androidStatusBar": {
			"backgroundColor": "#e8cdad",
			"barStyle": "dark-content",
			"translucent": false
		},
		"web": {
			"favicon": "./assets/favicon.png"
		},
		"plugins": ["expo-router", "expo-secure-store"],
		"extra": {
			"router": {
				"origin": false
			},
			"eas": {
				"projectId": "08053d74-6055-4984-9cf7-2c1d63844cb2"
			}
		},
		"owner": "vinnre"
	}
}
